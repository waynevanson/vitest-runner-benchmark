{
  "name": "@waynevanson/vitest-benchmark",
  "description": "Vitest runners and reporters for benchmarking",
  "author": {
    "name": "Wayne Van Son"
  },
  "version": "0.8.0",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@vitest/coverage-v8": "4.0.14",
    "@vitest/runner": "^4.0.12",
    "@vitest/ui": "4.0.14",
    "glob": "^13.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "peerDependencies": {
    "@vitest/runner": "^4.0.12",
    "vitest": "^4.0.12"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": false
    },
    "@vitest/runner": {
      "optional": false
    }
  },
  "type": "module",
  "exports": {
    "./reporter/bmf": {
      "types": "./dist/reporter/bmf.d.ts",
      "default": "./dist/reporter/bmf.js"
    },
    "./reporter/silent": {
      "types": "./dist/reporter/silent.d.ts",
      "default": "./dist/reporter/silent.js"
    },
    "./runner": {
      "types": "./dist/runner/index.d.ts",
      "default": "./dist/runner/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "test": "vitest run",
    "benchmark": "BENCHMARK=1 vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "valibot": "^1.2.0"
  }
}
