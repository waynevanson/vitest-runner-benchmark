{
  "name": "@waynevanson/vitest-benchmark",
  "description": "Vitest runners and reporters for benchmarking",
  "author": {
    "name": "Wayne Van Son"
  },
  "version": "0.3.0",
  "files": [
    "dist",
    "runner",
    "bmf-reporter"
  ],
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@types/picomatch": "^4.0.2",
    "@vitest/runner": "^4.0.14",
    "glob": "^13.0.0",
    "picomatch": "^4.0.3",
    "typescript": "^5.9.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "@vitest/runner": "^4.0.12",
    "vitest": "^4.0.12"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": false
    },
    "@vitest/runner": {
      "optional": false
    }
  },
  "type": "module",
  "exports": {
    "./reporter/bmf": {
      "types": "./dist/public/reporter/bmf.d.ts",
      "default": "./dist/public/reporter/bmf.js"
    },
    "./reporter/silent": {
      "types": "./dist/public/reporter/silent.d.ts",
      "default": "./dist/public/reporter/silent.js"
    },
    "./runner": {
      "types": "./dist/public/runner.d.ts",
      "default": "./dist/public/runner.js"
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "test": "vitest run"
  },
  "packageManager": "pnpm@10.22.0"
}
