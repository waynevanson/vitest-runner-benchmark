{
  "name": "@waynevanson/vitest-benchmark",
  "description": "Vitest runners and reporters for benchmarking",
  "author": {
    "name": "Wayne Van Son"
  },
  "version": "0.2.5",
  "files": [
    "dist",
    "runner",
    "bmf-reporter"
  ],
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@vitest/runner": "^4.0.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "peerDependencies": {
    "vitest": "^4.0.12",
    "@vitest/runner": "^4.0.12"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": false
    },
    "@vitest/runner": {
      "optional": false
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./bmf-reporter": {
      "types": "./dist/bmf-reporter.d.ts",
      "default": "./dist/bmf-reporter.js"
    },
    "./runner": {
      "types": "./dist/runner.d.ts",
      "default": "./dist/runner.js"
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "benchmark": "vitest run"
  },
  "packageManager": "pnpm@10.22.0"
}
