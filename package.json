{
  "name": "vitest-runner-benchmark",
  "version": "0.2.0",
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@vitest/runner": "^4.0.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "peerDependencies": {
    "vitest": "^4.0.12",
    "@vitest/runner": "^4.0.12"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": false
    },
    "@vitest/runner": {
      "optional": false
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./bmf-reporter": {
      "types": "./dist/bmf-reporter.d.ts",
      "default": "./dist/bmf-reporter.js"
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "benchmark": "vitest run --reporter ./dist/bmf-reporter.js"
  },
  "packageManager": "pnpm@10.22.0"
}
